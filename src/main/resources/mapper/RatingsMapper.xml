<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rec.mapper.RatingsMapper">

    <resultMap id="BaseResultMap" type="com.example.rec.domain.Ratings">
            <result property="user" column="user" jdbcType="VARCHAR"/>
            <result property="item" column="item" jdbcType="VARCHAR"/>
            <result property="rating" column="rating" jdbcType="DECIMAL"/>
            <result property="datetime" column="datetime" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        user,item,rating,
        datetime
    </sql>
</mapper>
